{
  "compilerOptions": {
    "target": "es5",
    "allowJs": false,
    "noImplicitAny": false,
    "allowUnreachableCode": true,
    "outDir": "assets/js",
    "listEmittedFiles": true,
    "declaration": true,
    "lib": ["dom", "es5", "ScriptHost", "es2015.promise"]
  },
  "include": [
    "assets/ts/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts"
  ]
}